{
	"editor.tabSize": 2,
	"editor.insertSpaces": false,
	"editor.fontSize": 16,
	"editor.fontFamily": "FiraCode Nerd Font Mono, Menlo, Monaco, 'Courier New', monospace",
	"editor.stickyScroll.enabled": false,
	"editor.formatOnSave": true,
	"editor.inlineSuggest.edits.allowCodeShifting": "never",

	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},
	"[ruby]": {
		"editor.tabSize": 2,
		"editor.rulers": [120]
	},

	"files.exclude": {
		".DS_Store": true,
		"*.log": true,
		"**/__pycache__": true,
		"**/.bundle": true,
		"**/.idea": true,
		"**/.next": true,
		"**/.pnpm-store": true,
		"**/.sass-cache": true,
		"**/cache": true,
		"**/coverage": true,
		"**/log": true,
		"**/node_modules": true,
		"**/node-compile-cache": true,
		"**/public/packs": true,
		"**/storage": true,
		"**/temp": true,
		"**/tmp": true
	},
	"files.associations": {
		".aliases": "shellscript",
		".zshrc-local": "shellscript",
		".zprofile-local": "shellscript"
	},
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.jsx": "${capture}.js",
		"*.tsx": "${capture}.ts",
		"tsconfig.json": "tsconfig.*.json",
		"package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
		"*.sqlite": "${capture}.${extname}-*",
		"*.db": "${capture}.${extname}-*",
		"*.sqlite3": "${capture}.${extname}-*",
		"*.db3": "${capture}.${extname}-*",
		"*.sdb": "${capture}.${extname}-*",
		"*.s3db": "${capture}.${extname}-*"
	},

	"terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
	"terminal.integrated.fontSize": 14,
	"terminal.explorerKind": "external",
	"terminal.external.osxExec": "Ghostty.app",

	"window.zoomLevel": 1,
	"window.commandCenter": true,
	"workbench.colorTheme": "Catppuccin Frapp√©",

	"git.openRepositoryInParentFolders": "never",
	"git.autofetch": true,

	"gitlens.views.worktrees.files.layout": "list",
	"gitlens.ai.model": "gitkraken",
	"gitlens.ai.vscode.model": "copilot:gpt-4o",

	"css.validate": false,
	"scss.validate": false,
	"less.validate": false,
	"stylelint.validate": [
		"css",
		"scss",
		"less",
		"postcss",
		"sass",
		"vue",
		"html",
		"markdown",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"stylelint.config": {
		"extends": ["stylelint-config-standard", "stylelint-config-tailwindcss"],
		"rules": {
			"at-rule-no-unknown": [
				true,
				{
					"ignoreAtRules": [
						"tailwind",
						"apply",
						"variants",
						"responsive",
						"screen"
					]
				}
			]
		}
	},

	"yaml.schemas": {
		"https://json.schemastore.org/github-workflow.json": "file:///Users/mel/Work/flexnet-worktrees/main/.github/workflows/*.yml"
	},

	"todo-tree.filtering.excludeGlobs": [
		"**/node_modules/**",
		"**/dist/**",
		"**/build/**",
		"**/coverage/**",
		"**/tmp/**",
		"**/vendor/**",
		"**/.git/**",
		"**/.bundle/**",
		"**/.next/**",
		"**/public/packs/**",
		"**/storage/**",
		"**/log/**",
		"**/.sass-cache/**",
		"**/.pnpm-store/**"
	],
	"todo-tree.general.tags": [
		"TODO",
		"BUG",
		"FIXME",
		"NOTE",
		"@todo",
		"@bug",
		"@fixme"
	],
	"todo-tree.highlights.customHighlight": {
		"TODO": {
			"icon": "check-circle",
			"foreground": "#ffffff",
			"background": "#4abb89",
			"iconColour": "#2ecc71",
			"type": "tag"
		},
		"BUG": {
			"icon": "bug",
			"foreground": "#ffffff",
			"background": "#e74c3c",
			"iconColour": "#ee0000",
			"type": "tag"
		},
		"FIXME": {
			"icon": "tools",
			"foreground": "#ffffff",
			"background": "#e67e22",
			"iconColour": "#f39c12",
			"type": "tag"
		},
		"NOTE": {
			"icon": "note",
			"foreground": "#ffffff",
			"background": "#3498db",
			"iconColour": "#3498db",
			"type": "tag"
		},
		"@todo": {
			"icon": "check-circle",
			"foreground": "#ffffff",
			"background": "#4abb89",
			"iconColour": "#2ecc71",
			"type": "tag"
		},
		"@bug": {
			"icon": "bug",
			"foreground": "#ffffff",
			"background": "#e74c3c",
			"iconColour": "#ee0000",
			"type": "tag"
		},
		"@fixme": {
			"icon": "tools",
			"foreground": "#ffffff",
			"background": "#e67e22",
			"iconColour": "#f39c12",
			"type": "tag"
		}
	},

	"redhat.telemetry.enabled": false,
	"supermaven.enable": {
		"*": true
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"github.copilot.nextEditSuggestions.enabled": false,
	"github.copilot.chat.agent.thinkingTool": true,
	"docker.lsp.experimental.vulnerabilityScanning": false,
	"docker.extension.experimental.composeCompletions": true,
	"workbench.editor.empty.hint": "hidden",

	"editor.detectIndentation": true,
	"files.trimTrailingWhitespace": true,
	"files.insertFinalNewline": true,
	"files.eol": "\n",

	"editor.rulers": [100],
	"editor.wordWrap": "off",

	"editor.formatOnSaveMode": "file",
	"editorconfig.generateAuto": true,
	"highlight.regexes": {
		"pulumi\\.interpolate`([\\s\\S]*?)`": {
			"language": "shellscript",
			"filterLanguageRegex": "typescript|javascript"
		},
		"(sh|bash)`([\\s\\S]*?)`": {
			"language": "shellscript",
			"filterLanguageRegex": "typescript|javascript"
		},
		"((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *TODO(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
			"filterFileRegex": ".*(?<!CHANGELOG.md)$",
			"decorations": [
				{
					"overviewRulerColor": "#ffcc00",
					"backgroundColor": "#ffcc00",
					"color": "#1f1f1f",
					"fontWeight": "bold"
				},
				{
					"backgroundColor": "#ffcc00",
					"color": "#1f1f1f"
				}
			]
		},
		"((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:FIXME|FIX|BUG|UGLY|DEBUG|HACK)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
			"filterFileRegex": ".*(?<!CHANGELOG.md)$",
			"decorations": [
				{
					"overviewRulerColor": "#cc0000",
					"backgroundColor": "#cc0000",
					"color": "#1f1f1f",
					"fontWeight": "bold"
				},
				{
					"backgroundColor": "#cc0000",
					"color": "#1f1f1f"
				}
			]
		},
		"((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:REVIEW|OPTIMIZE|TSC)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
			"filterFileRegex": ".*(?<!CHANGELOG.md)$",
			"decorations": [
				{
					"overviewRulerColor": "#00ccff",
					"backgroundColor": "#00ccff",
					"color": "#1f1f1f",
					"fontWeight": "bold"
				},
				{
					"backgroundColor": "#00ccff",
					"color": "#1f1f1f"
				}
			]
		},
		"((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:IDEA)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
			"filterFileRegex": ".*(?<!CHANGELOG.md)$",
			"decorations": [
				{
					"overviewRulerColor": "#cc00cc",
					"backgroundColor": "#cc00cc",
					"color": "#1f1f1f",
					"fontWeight": "bold"
				},
				{
					"backgroundColor": "#cc00cc",
					"color": "#1f1f1f"
				}
			]
		}
	},
	"github.copilot.selectedCompletionModel": "",
	"github.copilot.chat.followUps": "firstOnly",
	"github.copilot.chat.codesearch.enabled": true,
	"github.copilot.chat.completionContext.typescript.mode": "sidecar",
	"github.copilot.chat.editor.temporalContext.enabled": true,
	"github.copilot.chat.edits.temporalContext.enabled": true,
	"github.copilot.chat.languageContext.fix.typescript.enabled": true,
	"gitlens.advanced.messages": {
		"suppressLineUncommittedWarning": true
	},
	"docker.extension.enableComposeLanguageServer": false,
	"cursor.composer.shouldChimeAfterChatFinishes": true,
	"cursor.composer.usageSummaryDisplay": "always",
	"cursor.terminal.usePreviewBox": true,
	"diffEditor.ignoreTrimWhitespace": true,
	"gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"json.schemaDownload.enable": true,
	"application.shellEnvironmentResolutionTimeout": 90,
	"terminal.integrated.inheritEnv": true,
	"[dockercompose]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": "advanced",
		"editor.quickSuggestions": {
			"other": true,
			"comments": false,
			"strings": true
		},
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"[github-actions-workflow]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"diffEditor.hideUnchangedRegions.enabled": true,
	"cursor.agent_layout_browser_beta_setting": true,
	"editor.inlineSuggest.enabled": false
}
